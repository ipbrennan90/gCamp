  <div class = "page-header">
    <h1>
      Users
    </h1>
  </div>

  <% if flash[:success] %>
    <div class= "alert alert-success text-center">
      <%= flash[:success] %>
    </div>
  <% end %>

    <table class = "table">
      <thead>
        <tr>
          <th>Name</th>
          <th>Email</th>
        </tr>
      </thead>
      <% @users.each do |u| %>

        <tr>
          <td>
            <%=u.first_name+" "+u.last_name %>
          </td>
          <td>
            <%= u.email %>
          </td>
        </tr>


      <% end %>
  </table>
